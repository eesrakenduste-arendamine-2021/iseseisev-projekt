{
    "manifest_version": 2,

    "name": "Rakendus",
    "description": "Rakendus taidab paar lunka",
    "version": "0.0.1",
    "options_page": "options.html",

    "browser_action": {
        "default_icon": "e.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "storage"
    ],
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["jquery-3.6.0.min.js", "faker.min.js", "content.js"]
    }]
}